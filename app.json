{
  "expo": {
    "name": "Kamadehnu Milk",
    "slug": "milk_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ios-light.png",
    "scheme": "milkapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash-icon-light.png",
      "resizeMode": "contain",
      "backgroundColor": "#16a34a"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.amitbaskey99.milk_app",

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#16a34a"
      },

      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION"],

      "config": {
        "googleMaps": {
          "apiKey": "YOUR_GOOGLE_MAPS_API_KEY"
        }
      },

      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",

      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Milk App to access your location to find delivery address"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "79451892-dd40-46a1-82d5-c08076a9c923"
      }
    }
  }
}
